{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "strict": true,
    "declaration": false,
    "target": "es6",
    "sourceMap": true,
    "lib": ["es6"],
    "outDir": "./lib",
    "rootDir": "./src",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
        "@/*": [
            "*"
        ]
    }
  },
  "include": [
      "src/*.ts",
      "src/*.js",
      "src/*.json"
  ],
}
